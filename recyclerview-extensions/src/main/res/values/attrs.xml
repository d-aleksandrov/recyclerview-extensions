<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MultiRecycler">
        <attr name="direction">
            <enum name="vertical" value="1" />
            <enum name="horizontal" value="2" />
        </attr>

        <attr name="pagination_sensitive" format="integer" />
        <attr name="reverse" format="boolean" />
        <attr name="cells" format="integer" />

        <attr name="cell_preloader" format="reference" />
        <attr name="cell_error" format="reference" />
        <attr name="cell_end" format="reference" />

    </declare-styleable>
</resources>
